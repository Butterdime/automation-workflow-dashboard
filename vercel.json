{
  "version": 2,
  "outputDirectory": "dashboard/dist",
  "builds": [
    { "src": "api/server.js", "use": "@vercel/node" },
    { "src": "dashboard/dist/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/server.js" },
    { "src": "/(.*)", "dest": "/dashboard/dist/index.html" }
  ]
}